{
  "name": "test-jest-imports",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start-dev": "npm run build && npm run start:dev",
    "start-debug": "npm run build && npm run start:debug",
    "build": "rimraf dist && tsc -p tsconfig.build.json && copyfiles \"src/config/errors/*.yaml\" dist",
    "prebuild": "ts-node ./src/generate-typings",
    "exportgraphql": "copyfiles \"src/**/*.graphql\" dist",
    "format": "prettier --write \"src/**/*.ts\"",
    "start:dev": "env-cmd -f ./dev.env nest start --watch",
    "start:debug": "env-cmd -f ./dev.env tsc-watch -p tsconfig.build.json --onSuccess nest start --debug --watch",
    "start": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./jest.config.js"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.13",
    "@nestjs/core": "^7.6.13",
    "@nestjs/graphql": "7.10.3",
    "@nestjs/platform-express": "^7.6.13",
    "apollo-server-core": "^2.21.2",
    "apollo-server-express": "^2.17.0",
    "camelcase-keys": "^6.0.1",
    "class-validator": "^0.13.1",
    "codepage": "1.14.0",
    "date-fns": "^2.21.1",
    "dotenv": "^8.2.0",
    "graphql": "^14.7.0",
    "graphql-tools": "^4.0.8",
    "jest-config": "^27.5.1",
    "joi": "^17.3.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.6",
    "@nestjs/schematics": "^7.2.7",
    "@nestjs/testing": "^7.6.13",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.2",
    "ts-loader": "^8.0.17",
    "ts-node": "^9.1.1",
    "tsc-watch": "2.2.1",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5",
    "tslint": "5.16.0",
    "@types/cls-hooked": "^4.3.0",
    "copyfiles": "~2.1.1"
  }
}
